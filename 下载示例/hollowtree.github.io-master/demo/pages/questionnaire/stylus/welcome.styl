.page-welcome
    table
        width:80%
        margin 80px auto 0
        background #fff
        tr
            text-align: center
            th
                line-height: 40px
            th:nth-child(1)
                width 30%
            th:nth-child(2)
                width 30%
            th:nth-child(3)
                width 10%
            th:nth-child(4)
                width 30%
            td
                line-height: 34px
                a
                    display: inline-block
                    line-height: 1
                    margin: 5px 10px
                    border: 1px solid #666
                    padding: 1px 3px